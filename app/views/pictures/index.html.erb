
<div class="jumbotron p-4 p-md-5 text-white rounded bg-dark">

<h1 class="display-4 font-italic">Welcome to Events <%=current_user.username%></h1>
<h3><p class="lead my-3">Share your precious moments with us</p></h3>
</div>


<p class="text-info">Popular tags:
<%@tags.each do |tag|%>
    <svg class="bi bi-tag" width="1em" height="1em" viewBox="0 0 16 16" fill="#6cd3e6" xmlns="http://www.w3.org/2000/svg">
    <path fill-rule="evenodd" d="M.5 2A1.5 1.5 0 012 .5h4.586a1.5 1.5 0 011.06.44l7 7a1.5 1.5 0 010 2.12l-4.585 4.586a1.5 1.5 0 01-2.122 0l-7-7A1.5 1.5 0 01.5 6.586V2zM2 1.5a.5.5 0 00-.5.5v4.586a.5.5 0 00.146.353l7 7a.5.5 0 00.708 0l4.585-4.585a.5.5 0 000-.708l-7-7a.5.5 0 00-.353-.146H2z" clip-rule="evenodd"/>
    <path fill-rule="evenodd" d="M2.5 4.5a2 2 0 114 0 2 2 0 01-4 0zm2-1a1 1 0 100 2 1 1 0 000-2z" clip-rule="evenodd"/>
  </svg><%=link_to tag.tag_name, tag_path(tag)%>
<%end%>
</p>

<div class="parent">
<%@pictures.each do |pic|%>
  <%=link_to picture_path(pic) do%>
   
   <img class="photo" src=<%=pic.url%> style="background-image: url(<%=pic.url%>)" />
    <%end%>
  <%end%>

</div>


    

