<h1>Welcome to Events</h1>
<h3>Please login</h3>
<%=flash[:message]%>
<%=form_for :auth do |f|%>
<%=f.label :username%>
<%=f.text_field :username%><br><br>
<%=f.label :password%>
<%=f.password_field :password%><br><br>
<%=f.submit "login"%>
<%end%>

<%=button_to "signup", signup_path, method: :get%>
