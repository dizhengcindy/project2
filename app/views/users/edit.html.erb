<h1>Edit user profile</h1>
<%=form_for @user do |f|%>
    <%=f.label :username%>
    <%=f.text_field :username%><br><br>
    <%=f.label :email%>
    <%=f.text_field :email%><br><br>
    <%=f.submit%>
    <%end%>
    <%=button_to "Delete", {:controller =>:users, :action=>'destroy', :id => @user.id}, method: :delete%>